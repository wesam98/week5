#include <stdio.h>
#include <stdlib.h>
int sum (int *num1,int *num2);
int main()
{
    int num1,num2;
    printf("enter two numbers ");
    scanf("%d %d",&num1 ,&num2);
    printf("the sum of 2 numbers is:%d",sum(&num1,&num2));
}
int sum (int *num1, int *num2)
{
    return *num1 + *num2;
}
